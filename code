#include <iostream>
#include <vector>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {
    int n = 8, s = 0, k = 0;
    vector <int> a(n);
    vector <int> b(n);
    for (int i = 0; i < n; i++) {
        cin >> a[i] >> b[i];
    }
    for (int i = -1; i < 1; i += 2) {
        for (int j = -1; j < 1; j += 2) {
            for (int f = 0; f < n; ++f) {
                for (int m = 0; m < n; ++m) {
                    //cout << a[f] + i << ' ' << a[m] << ' ' << b[f] + j << ' ' << b[m];
                    if (((a[f] + i) == a[m]) and ((b[f] + j) == b[m])) {
                        k += 1;
                    }
                    else {
                        s += 1;                        
                    }
                }
            }
        }
    }
    //cout << k;
    if (k == 0) {
        cout << "NO";
    }
    else {
        cout << "YES";
    }
}
